<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.nightlabs.history.StartupListener">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.nightlabs.history.EditorHistoryBackActionHandler"
            commandId="org.eclipse.ui.navigate.backwardHistory">
      </handler>
      <handler
            class="org.nightlabs.history.EditorHistoryForwardActionHandler"
            commandId="org.eclipse.ui.navigate.forwardHistory">
      </handler>
   </extension>

</plugin>
